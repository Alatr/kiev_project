block meta
  title= title_page
  meta(name='description', content='')
block head
    link(rel='stylesheet', href="./assets/styles/main.min.css")
block scripts
  script(defer src='./assets/scripts/vendors.bundle.js')
  script(defer src='./assets/scripts/index.bundle.js')
  script(defer src='./assets/scripts/libs.js')
  script(defer src='./assets/scripts/index.js')
  script(defer src='./assets/scripts/about.js')
block content
  .page-part
    include ../includes/breadcrumbs.pug
    h1.page-title #{title_page}
    +iconTitleDecor('blue','Історія')
    .about-double-side-block
      .about-double-side-block__left
        +img('about/block1-bg.jpg').about-double-side-block__bg(data-block-img)
        .subtitle-with-vertical-decor
          .subtitle Що значить КИЇВПРОЕКТ для Києва?
      .about-double-side-block__right
        ul
          each el in block1List
            +infoItem(el[0], el[1], 'half-opacity')
  .page-part 
    .about-slider
      .about-slider__line-left
      .about-slider__line-right
      .about-slider__title
        .subtitle.black.bold Будівлі, спроєктовані у КИЇВПРОЕКТІ
      .about-slider__slides.swiper-container 
        .swiper-container__bg
        .swiper-wrapper
          img(src="./assets/images/about/slide1.jpg").swiper-slide
          img(src="./assets/images/about/slide1.jpg").swiper-slide
          img(src="./assets/images/about/slide1.jpg").swiper-slide
          img(src="./assets/images/about/slide1.jpg").swiper-slide
          img(src="./assets/images/about/slide1.jpg").swiper-slide
          img(src="./assets/images/about/slide1.jpg").swiper-slide
      +btn('')(data-prev)
      +btn('')(data-next)
      ul.about-slider__text
        each val in aboutSliderText
          li !{val}
  .page-part
    +iconTitleDecor('green','Натхнення')
    .about-double-side-block.about-double-side-block3
      .about-double-side-block__left
        +img('about/block3-bg.jpg').about-double-side-block__bg
      .about-double-side-block__right
        ul
          each el in block3List
            +infoItem(el[0], el[1], 'half-opacity')
