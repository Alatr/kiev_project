
extends ../layout/main
block variable
  -
    var title_page = 'Все про  Kyїvproekt'
    var id_page = 'id-page-about'
    var class_page = 'class-page-about'
    var breadcrumbsSubitem = [
      {
        label: 'About',
        link: pagesLink[0].about
      }
    ]
    var block1List = [
      ['<span class="bold">Тут були розроблені</span> всі післявоєнні генплани столиці.','about-block1-icon-1'],
      ['<span class="bold">Це місце народження київського модернізму,</span> який став символом нового архітектурного мислення.','about-block1-icon-2'],
      ['<span class="bold">Тут сформувалася плеяда талановитих фахівців,</span> які багато в чому визначили вигляд міста.','about-block1-icon-3'],
      ['<span class="bold">Тут проєктувалися всі</span> знакові будівлі післявоєнного Києва.','about-block1-icon-4'],
    ]
    var block3List = [
      ['<span class="bold">Проводячи паралелі у світовому контексті,</span> ми звертаємось до однієї з найвизначніших архітектурних шкіл Європи та світу. ','about-block3-icon1'],
      ['<span class="bold">Це Bauhaus — вища школа будівництва</span> та художнього конструювання, яка існувала у Німеччині з 1919 по 1933 роки. ','about-block3-icon2'],
      ['<span class="bold">Ідеї та принципи Bauhaus</span> поширились світом і стали поворотною точкою в архітектурі, мистецтві та дизайні. Цей надихаючий вплив ми відчуваємо й сьогодні.','about-block3-icon3'],
    ]
    var renovationTextBlocks = [
      ['<span class="bold">Наша мета</span> – створити у центрі Києва нову екосистему, яка поєднає в собі архітектуру та дизайн, мистецтво та технології, освіту та бізнес. ','renovation-decor1'],
      ['<span class="bold">Це місце для тих,</span> хто хоче бути там, де народжується все нове, прогресивне та модне.','renovation-decor2'],
      ['<span class="bold">Це новий центр тяжіння для всіх,</span> хто прагне змінювати довкілля та готовий змінюватися сам. Для талантів, новаторів, візіонерів.','renovation-decor3'],
      ['<span class="bold">Для тих, хто прагне</span> бути безпосереднім учасником найцікавіших і найважливіших подій міста та країни.','renovation-decor4'],
    ]
    var renovationList = [
      ['<span class="bold">Принцип функціоналізму</span> – архітектурний образ будинку визначається чітким розумінням його майбутніх функцій','about-renovation-list1','renovation-decor1'],
